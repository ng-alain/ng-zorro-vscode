{
  "version": "0.2.0",
  "configurations": [{
    "type": "node",
    "request": "launch",
    "name": "Resources",
    "program": "${workspaceFolder}/scripts/resource/main.js"
  },{
    "type": "node",
    "request": "launch",
    "name": "Snippet",
    "program": "${workspaceFolder}/scripts/snippet.js"
  },
    {
      "name": "Extension",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/out/**/*.js"
      ]
    }
  ]
}